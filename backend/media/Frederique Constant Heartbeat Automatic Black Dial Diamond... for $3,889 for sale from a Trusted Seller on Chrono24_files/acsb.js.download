            let $icon = EJS.elements.create('span', {
                'class' : 'acsb-trigger-icon'
            }, icons[state.widget.triggerIcon] ? icons[state.widget.triggerIcon] : icons.people);

            if( config.isIe ) {
                $icon = EJS.elements.create('span', {
