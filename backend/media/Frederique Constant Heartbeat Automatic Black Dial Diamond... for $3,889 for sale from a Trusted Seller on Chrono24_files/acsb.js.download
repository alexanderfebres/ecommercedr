            ja : 'ウェブサイトを読み込んでいます...',
            it : 'Caricamento sito Web in corso...',
        };
        const loading   = setInterval(() => {
            if( ! document.body ) {
                return;
            }

            clearTimeout(loading);
            EJS.elements.attrs(document.body, {
                'role'       : 'text',
                'aria-label' : loadTexts[params.language] ? loadTexts[params.language] : loadTexts.en
            });
